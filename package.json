{
  "name": "inst-node-jwt",
  "version": "5.2.1",
  "description": "express JWT middleware",
  "main": "lib/auth.js",
  "files": [
    "lib"
  ],
  "nyc": {
    "all": true,
    "lines": 80,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js"
    ]
  },
  "scripts": {
    "audit": "nsp check --threshold 3.99",
    "coverage:check": "cross-env NODE_ENV=test nyc check-coverage",
    "coverage:run": "cross-env NODE_ENV=test nyc mocha 'test/**/*.js'",
    "lint:check": "eslint src/ test/",
    "lint:fix": "eslint --fix src/ test/",
    "build": "babel src -d lib",
    "prepublishOnly": "yarn build",
    "test": "NODE_ENV=test mocha 'test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "ssh://gerrit.instructure.com:29418/node-jwt"
  },
  "keywords": [
    "node",
    "express",
    "jwt"
  ],
  "author": "Xander Moffatt, Andrew Huff",
  "license": "ISC",
  "dependencies": {
    "consul": "^0.29.0",
    "jsonwebtoken": "^8.2.1",
    "lodash.mapvalues": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nsp": "^3.2.1",
    "nyc": "^11.0.3",
    "prettier": "^1.11.1",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  }
}
