{
  "name": "inst-node-jwt",
  "version": "4.1.0",
  "description": "express JWT middleware",
  "main": "src/auth.js",
  "nyc": {
    "all": true,
    "lines": 80,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js"
    ]
  },
  "scripts": {
    "coverage:check": "cross-env NODE_ENV=test nyc check-coverage",
    "coverage:run": "cross-env NODE_ENV=test nyc mocha 'test/**/*.js'",
    "lint:check": "eslint src/ test/ && prettier --no-semi --trailing-comma es5 --list-different 'src/**/*.js' 'test/**/*.js'",
    "lint:fix": "eslint --fix src/ test/ && prettier --no-semi --trailing-comma es5 --write 'src/**/*.js' 'test/**/*.js'",
    "test": "NODE_ENV=test mocha 'test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "ssh://gerrit.instructure.com:29418/node-jwt"
  },
  "keywords": [
    "node",
    "express",
    "jwt"
  ],
  "author": "Xander Moffatt, Andrew Huff",
  "license": "ISC",
  "dependencies": {
    "consul": "^0.29.0",
    "jsonwebtoken": "^7.4.1",
    "lodash.mapvalues": "^4.6.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "prettier": "^1.5.2",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  }
}
